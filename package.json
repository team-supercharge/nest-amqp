{
  "name": "@team-supercharge/nest-amqp",
  "version": "1.3.0",
  "description": "AMQP 1.0 module for Nest framework",
  "author": "Supercharge",
  "license": "MIT",
  "keywords": [
    "amqp",
    "queue",
    "nest"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/team-supercharge/nest-amqp"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "prebuild:prod": "rimraf dist",
    "build": "nest build",
    "build:prod": "nest build -p tsconfig.prod.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "release": "standard-version",
    "codecov": "codecov",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "rhea-promise": "1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@nestjs/cli": "7.5.6",
    "@nestjs/common": "7.6.13",
    "@nestjs/core": "7.6.13",
    "@nestjs/schematics": "7.2.9",
    "@nestjs/testing": "7.6.13",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.33",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "class-transformer": "0.3.2",
    "class-validator": "0.12.2",
    "codecov": "3.8.1",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "json": "10.0.0",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.6",
    "standard-version": "9.1.1",
    "ts-jest": "26.5.3",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0",
    "class-transformer": "^0.3.0",
    "class-validator": "^0.12.0"
  }
}
