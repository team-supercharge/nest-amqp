{
  "name": "@team-supercharge/nest-amqp",
  "version": "3.1.0",
  "description": "AMQP 1.0 module for Nest framework",
  "author": "Supercharge",
  "license": "MIT",
  "keywords": [
    "amqp",
    "queue",
    "nest"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/team-supercharge/nest-amqp"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "prebuild:prod": "rimraf dist",
    "build": "nest build",
    "build:prod": "nest build -p tsconfig.prod.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "release": "standard-version",
    "codecov": "codecov",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,examples}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:clear": "jest --clearCache true --clearMocks true && jest",
    "check": "npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "rhea-promise": "2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@nestjs/cli": "8.2.4",
    "@nestjs/common": "8.4.3",
    "@nestjs/core": "8.4.3",
    "@nestjs/schematics": "8.0.8",
    "@nestjs/testing": "8.4.3",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.5",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.2",
    "codecov": "3.8.3",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "4.3.8",
    "jest": "27.0.6",
    "json": "11.0.0",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0",
    "standard-version": "9.3.1",
    "ts-jest": "27.0.4",
    "ts-loader": "9.2.4",
    "ts-node": "10.1.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "class-transformer": "^0.3.0 || ^0.4.0",
    "class-validator": ">=0.13.2"
  }
}
