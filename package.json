{
  "name": "@team-supercharge/nest-amqp",
  "version": "3.6.4",
  "description": "AMQP 1.0 module for Nest framework",
  "author": "Supercharge",
  "license": "MIT",
  "keywords": [
    "amqp",
    "queue",
    "nest"
  ],
  "engines": {
    "node": ">16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/team-supercharge/nest-amqp"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "prebuild:prod": "rimraf dist",
    "build": "nest build",
    "build:prod": "nest build -p tsconfig.prod.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "release": "commit-and-tag-version",
    "codecov": "codecov",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,examples}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:clear": "jest --clearCache true --clearMocks true && jest",
    "check": "npm run lint && npm run build && npm run test",
    "prepare": "husky"
  },
  "dependencies": {
    "rhea-promise": "^3.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@nestjs/testing": "11.0.11",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.9",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "codecov": "3.8.3",
    "commit-and-tag-version": "12.5.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "json": "11.0.0",
    "prettier": "3.5.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "ts-jest": "29.2.6",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "type-fest": "4.37.0",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0",
    "@nestjs/core": ">=10.0.0",
    "class-transformer": ">=0.3.0",
    "class-validator": ">=0.13.2"
  }
}
